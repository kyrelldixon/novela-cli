{
  "name": "@kyrelldixon/novela-cli",
  "description": "Create local Novela post boilerplate",
  "version": "0.0.2",
  "author": "Kyrell Dixon <kyrell@kyrelldixon.com>",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "publish": "npm publish --access public"
  },
  "bin": {
    "novela-cli": "./bin/run"
  },
  "bugs": "https://github.com/kyrelldixon/novela-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.2",
    "inquirer": "^7.0.3",
    "inquirer-path": "^1.0.0-beta5",
    "js-yaml": "^3.13.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kyrelldixon/novela-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "novela-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kyrelldixon/novela-cli",
  "types": "lib/index.d.ts"
}
